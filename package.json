{
  "name": "react-express",
  "version": "0.1.0",
  "private": true,
  "description": "Express server serving create-react-app",
  "author": "Patrick Lin <paatricklinn@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "engines": {
    "node": "~6.9.1",
    "npm": ">=3.10.0"
  },
  "scripts": {
    "prestart": "npm run -s build",
    "start": "node dist/index.js",
    "build": "npm run clean && mkdir -p dist && babel server -s -D -d dist",
    "start-react": "react-scripts start",
    "build-react": "rm -rf public/build && react-scripts build && mv build public",
    "dev": "nodemon src/index.js --exec \"node -r dotenv/config -r babel-register\"",
    "clean": "rimraf dist",
    "test-react": "react-scripts test --env=jsdom",
    "test": "jest --watch",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.15",
    "rimraf": "^2.6.2"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "jest": "^21.2.1",
    "nodemon": "^1.12.1",
    "supertest": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  }
}
